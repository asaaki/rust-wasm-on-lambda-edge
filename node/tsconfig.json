{
  "compilerOptions": {
    "allowJs": false,
    "allowUnreachableCode": false,
    "allowUnusedLabels": false,
    "lib": ["ES2020"],
    "module": "ES2020",
    "moduleResolution": "node",
    "newLine": "LF",
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "outDir": "./built",
    "sourceMap": false,
    "strict": true,
    "target": "ES2020"
  },
  "exclude": [
    "built",
    "node_modules"
  ]
}
